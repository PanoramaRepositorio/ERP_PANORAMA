<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="reporteproducto"
    targetNamespace="http://tempuri.org/reportelineaproducto.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/reportelineaproducto.xsd"
    xmlns:mstns="http://tempuri.org/reportelineaproducto.xsd"
