      <xs:attribute name="DescMarca" type="xs:string" />
      <xs:attribute name="DescProcedencia" type="xs:string" />
      <xs:attribute name="Periodo" type="xs:int" />
      <xs:attribute name="PrecioABSoles" type="xs:decimal" />
      <xs:attribute name="PrecioCDSoles" type="xs:decimal" />
