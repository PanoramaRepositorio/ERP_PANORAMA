      <xs:attribute name="IdMaterial" type="xs:int" />
      <xs:attribute name="IdMarca" type="xs:int" />
      <xs:attribute name="IdProcedencia" type="xs:int" />
      <xs:attribute name="NombreProducto" type="xs:string" />
      <xs:attribute name="Descripcion" type="xs:string" />
